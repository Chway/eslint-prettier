{
  "env": {
    "browser": true,
    "webextensions": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "extends": ["xo", "xo/browser", "xo-space", "xo-space/browser", "plugin:unicorn/all", "prettier"],
  "rules": {
    "unicorn/no-keyword-prefix": "off",
    "unicorn/prefer-top-level-await": "off",
    "unicorn/no-null": "off",
    "unicorn/no-array-reduce": "off",
    "unicorn/prevent-abbreviations": "off",
    "unicorn/filename-case": "off",
    "unicorn/no-unsafe-regex": "off",
    "spaced-comment": "off",
    "capitalized-comments": "off",
    "curly": "off"
  }
}
